//class component
import React, { Component } from 'react'

 class Hello extends Component {
  render() {
    return (
      <div><h1>Hello,Welcome to React</h1></div>
    )
  }
}

//Functional componet 
import React from "react";

function Hello (){
    return(
    <div><h1>Welcome learner</h1>
    </div>
    );
}

export default Hello;


